print('q A hipotenusa vai medir {}'.format(hi))
2
import math
co = float (input('comprimento do cateto oposto: '))
ca = float (input('comprimento do cateto adiacente: '))
hi = math.hypot(co, ca)
print ('A hipotenusa vai medir {}'.format(hi))
